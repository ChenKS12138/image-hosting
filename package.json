{
  "name": "qy-graph-bed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.9",
    "axios": "^0.18.0",
    "node-sass": "^4.10.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "reqwest": "^2.0.5"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
